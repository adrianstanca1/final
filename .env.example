# Copy this file to .env.local and fill in the secrets before running locally.
# When deploying to Vercel set the same variables in the project settings.

# Public client-side API key for Gemini. Required for AI powered features.
VITE_GEMINI_API_KEY=

# Firebase Configuration - Required for authentication
VITE_FIREBASE_API_KEY=your_firebase_api_key_here
VITE_FIREBASE_AUTH_DOMAIN=your-project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your-project-id
VITE_FIREBASE_STORAGE_BUCKET=your-project.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=your_sender_id
VITE_FIREBASE_APP_ID=your_app_id
