{
  "name": "asagents---final",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "install": "node ./scripts/install-deps.mjs",
    "dev": "vite",
    "dev:vercel": "vercel dev",
    "mm:py": "uvicorn mm_service.main:app --reload --port 8010",
    "mm:verify": "npm run type-check && npm run test && npm run build",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest --coverage",
    "test:db": "tsx src/tests/database-integration.ts",
    "type-check": "tsc --noEmit",
    "lint": "echo 'Linting not configured yet'",
    "deploy": "node scripts/deploy.js",
    "deploy:staging": "node scripts/deploy.js staging",
    "deploy:production": "node scripts/deploy.js production",
    "deploy:vercel": "node scripts/deploy.js production vercel",
    "deploy:netlify": "node scripts/deploy.js production netlify",
    "deploy:docker": "node scripts/deploy.js production docker",
    "deploy:dry-run": "node scripts/deploy.js production vercel --dry-run",
    "start": "npm run preview",
    "analyze": "ANALYZE_BUNDLE=true npm run build"
  },
  "dependencies": {
    "@google/genai": "^1.17.0",
    "@hookform/resolvers": "^3.3.2",
    "@radix-ui/react-progress": "^1.0.3",
    "@radix-ui/react-tabs": "^1.0.4",
    "@supabase/supabase-js": "^2.39.3",
    "@tanstack/react-query": "^5.17.0",
    "date-fns": "^4.1.0",
    "leaflet": "1.9.4",
    "lucide-react": "^0.263.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.48.2",
    "react-leaflet": "4.2.1",
    "use-supercluster": "0.4.0",
    "zod": "^3.22.4",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@types/node": "^22.14.0",
    "terser": "^5.44.0",
    "tsx": "^4.6.2",
    "typescript": "~5.8.2",
    "vercel": "^48.1.4",
    "vite": "^6.2.0",
    "vitest": "^3.2.4"
  }
}
