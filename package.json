{
  "name": "asagents---final",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "install": "node ./scripts/install-deps.mjs",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest --coverage",
    "type-check": "tsc --noEmit",
    "lint": "echo 'Linting not configured yet'",
    "deploy": "npm run build",
    "deploy:ionos": "node scripts/deploy.js production ionos",
    "deploy:ionos-dry": "node scripts/deploy.js production ionos --dry-run",
    "start": "npm run preview",
    "analyze": "ANALYZE_BUNDLE=true npm run build",
    "deploy:production": "chmod +x deploy.sh && ./deploy.sh",
    "deploy:vercel": "chmod +x deploy.sh && ./deploy.sh --platform vercel",
    "deploy:railway": "chmod +x deploy.sh && ./deploy.sh --platform railway",
    "deploy:full": "chmod +x deploy.sh && ./deploy.sh --platform both",
    "deploy:webspace": "node deploy-sftp.js",
    "build:webspace": "cp .env.webspace .env.local && npm run build",
    "test:production": "node test-production.js",
    "setup:production": "cd server && node scripts/production-setup.js",
    "build:production": "NODE_ENV=production npm run build",
    "health:check": "curl -f http://localhost:4000/api/system/health || exit 1"
  },
  "dependencies": {
    "@google/genai": "^1.17.0",
    "@supabase/supabase-js": "^2.58.0",
    "date-fns": "^4.1.0",
    "leaflet": "1.9.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-leaflet": "4.2.1",
    "use-supercluster": "0.4.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/leaflet": "^1.9.0",
    "@types/node": "^22.14.0",
    "@types/react": "^19.1.14",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^4.0.0",
    "jsdom": "^27.0.0",
    "ssh2": "^1.17.0",
    "terser": "^5.44.0",
    "typescript": "~5.8.2",
    "vite": "^6.2.0",
    "vitest": "^3.2.4"
  }
}
