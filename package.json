{
  "name": "asagents---final",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:vercel": "vercel dev",
    "dev:mm": "concurrently \"npm run dev\" \"npm run mm:py\" \"npm run dev:services\"",
    "dev:services": "tsx watch services/index.ts",
    "mm:py": "uvicorn mm_service.main:app --reload --port 8010",
    "mm:verify": "npm run type-check && npm run test && npm run build:multimodal",
    "build": "vite build",
    "build:multimodal": "node scripts/build-multimodal.js",
    "build:frontend": "vite build",
    "build:api": "tsc -p tsconfig.api.json",
    "build:services": "tsc -p tsconfig.services.json",
    "build:types": "tsc -p tsconfig.base.json --emitDeclarationOnly --declaration --outDir dist/types",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest --coverage",
    "test:db": "tsx src/tests/database-integration.ts",
    "type-check": "tsc --noEmit --skipLibCheck",
    "type-check:api": "tsc -p tsconfig.api.json --noEmit",
    "type-check:services": "tsc -p tsconfig.services.json --noEmit",
    "lint": "eslint . --ext .ts,.tsx",
    "deploy": "node scripts/deploy.js",
    "deploy:staging": "node scripts/deploy.js staging",
    "deploy:production": "node scripts/deploy.js production",
    "deploy:vercel": "node scripts/deploy.js production vercel",
    "deploy:netlify": "node scripts/deploy.js production netlify",
    "deploy:docker": "node scripts/deploy.js production docker",
    "deploy:surge": "node scripts/deploy.js production surge",
    "deploy:dry-run": "node scripts/deploy.js production vercel --dry-run",
    "start": "concurrently \"npm run preview\" \"npm run mm:py\" \"node dist-services/services/index.js\"",
    "start:frontend": "npm run preview",
    "start:services": "node dist-services/services/index.js",
    "analyze": "ANALYZE_BUNDLE=true npm run build",
    "deploy:gh": "npm run build:multimodal && gh-pages -d dist",
    "docker:build": "docker build -t construction-app .",
    "docker:build:dev": "docker build --target development -t construction-app:dev .",
    "docker:run": "docker run -p 8080:80 construction-app",
    "docker:run:dev": "docker run -p 3000:3000 -v $(pwd):/app construction-app:dev",
    "surge:deploy": "npm run build:multimodal && npx surge dist construction-app-final.surge.sh",
    "surge:teardown": "npx surge teardown construction-app-final.surge.sh"
  },
  "dependencies": {
    "@google/genai": "^1.17.0",
    "@hookform/resolvers": "^3.3.2",
    "@prisma/client": "^6.16.2",
    "@radix-ui/react-progress": "^1.0.3",
    "@radix-ui/react-tabs": "^1.0.4",
    "@supabase/supabase-js": "^2.39.3",
    "@tanstack/react-query": "^5.17.0",
    "@types/dotenv": "^6.1.1",
    "bcryptjs": "^3.0.2",
    "compression": "^1.8.1",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "leaflet": "1.9.4",
    "lucide-react": "^0.263.1",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.48.2",
    "react-leaflet": "4.2.1",
    "redis": "^5.8.2",
    "socket.io": "^4.8.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "use-supercluster": "0.4.0",
    "winston-daily-rotate-file": "^5.0.0",
    "zod": "^3.22.4",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.8.1",
    "@types/express": "^5.0.3",
    "@types/helmet": "^0.0.48",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/leaflet": "^1.9.20",
    "@types/morgan": "^1.9.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^22.18.6",
    "@types/react": "^18.3.24",
    "@types/react-dom": "^18.3.7",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@typescript-eslint/eslint-plugin": "^7.7.0",
    "@typescript-eslint/parser": "^7.7.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "gh-pages": "^6.3.0",
    "jsdom": "^27.0.0",
    "terser": "^5.44.0",
    "tsx": "^4.6.2",
    "typescript": "~5.8.2",
    "vercel": "^25.2.0",
    "vite": "^6.2.0",
    "vitest": "^3.2.4"
  }
}
