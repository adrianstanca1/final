{
  "name": "asagents-backend",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "start": "node --env-file=.env dist/index.js",
    "build": "tsc -p tsconfig.json",
    "lint": "eslint \"src/**/*.ts\"",
    "hash-password": "tsx scripts/hash-password.ts",
    "migrate": "node scripts/runMigrations.js",
    "migrate:dry-run": "node scripts/runMigrations.js --dry-run",
    "db:setup": "npm run migrate",
    "test": "jest tests/enhanced-backend.test.js",
    "test:coverage": "jest tests/enhanced-backend.test.js --coverage",
    "debug": "tsx --inspect src/index.ts",
    "health-check": "curl -f http://localhost:4000/api/system/health || exit 1"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dayjs": "^1.11.13",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "mysql2": "^3.11.4",
    "pino": "^9.6.0",
    "pino-pretty": "^13.1.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.14.0",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.18.0",
    "@typescript-eslint/parser": "^8.18.0",
    "eslint": "^9.20.1",
    "jest": "^30.1.3",
    "supertest": "^7.1.4",
    "tsx": "^4.19.2",
    "typescript": "^5.8.2"
  }
}
